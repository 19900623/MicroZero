{
  "urls": "http://*:5050",
  "ASPNETCORE_ENVIRONMENT_": "Development",
  "AppName": "WebMonitor",
  "Zero": {
    "Global": {
      "ZeroAddress": "192.168.1.192",
      "ZeroMonitorPort": 8001,
      "ZeroManagePort": 8000,
      "SpeedLimitModel": "ThreadCount",
      "MaxWait": 512,
      "TaskCpuMultiple": 1,
      "ServiceKey": "agebull",
      "CanRaiseEvent": true
    },
    "WebMonitor": {
      "StationName": "WebMonitor",
      "ShortName": "wm"
    }
  },
  "ConnectionStrings": {
    /*数据库连接*/
    "ZeroTrace": "Database=db_zero_trace;Data Source=192.168.1.192;User Id=root;Password=123456;Persist Security Info=true;pooling=true;SslMode = none;CharSet=utf8;port=3306"
  },
  "WebSocket": {
    "Folders": [ "config", "status", "kline", "api", "plan_notify", "event", "trace_flow" ]
  },
  "LogRecorder": {
    "monitor": false,
    "sql": false,
    "level": "Warning",
    "console": true
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*"
}
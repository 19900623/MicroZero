{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "server" : {
    "urls": "http://*:5001"
  },
  /*ZeroNet站点配置（不要改）*/
  "Zero": {
    "Global": {
      /*ZeroCenter主机IP地址*/
      "ZeroAddress": "192.168.240.132",
      /*ZeroCenter监测端口号*/
      "ZeroMonitorPort": 8001,
      /*ZeroCenter管理端口号*/
      "ZeroManagePort": 8000,
      /*限速模式（0 单线程 1 按线程数限制 2 按等待数限制）*/
      "SpeedLimitModel": "ThreadCount",
      /*最大等待数*/
      "MaxWait": 512,
      /*最大Task与Cpu核心数的倍数关系*/
      "TaskCpuMultiple": 1,
      "ServiceKey": "*"
    },
    "Station": {
      "StationName": "HttpRoute",
      "ShortName": "route",
      "AddInPath": "AddIn"
    }
  },
  "LogRecorder": {
    /*是否记录跟踪日志*/
    "monitor": true
  },
  "ConnectionStrings": {
    /*校验Auth的Redis的地址*/
    "Redix": "127.0.0.1:6379"
  },
  "HttpRooter": {
    /*系统设置*/
    "sysConfig": {
      /*是否开启ZMQ网络（不要改）*/
      "FireZero": true,
      "LogMonitor": true,
      /*日志目录*/
      "LogPath": "c:\\log\\route",
      /*HTTP调用超时*/
      "HttpTimeOut": 30000,
      /*短信超时报警的时长(毫秒)*/
      "WaringTime": 2000,
      /*内容页地址*/
      "ContextHost": "http://api.yizuanbao.cn:8080/"
    },
    /*路由信息，缺省地址必须要有，即找不到时使用的地址*/
    "route": {
      /*缺省地址* /
    "Default": {
      "hosts": [ "Http://127.0.0.1:8080/" ]
    },
    /*User对应的地址* /
    "GoodLin-User-External-Api": {
      "hosts": [ "http://172.16.20.142:8081/" ]
    },
    /*OAuth对应的地址* /
    "GoodLin-OAuth-Api": {
      "hosts": [ "http://172.16.20.142/" /*,"http://localhost:8080/GoodLin-OAuth-Api/"* /]
    }*/
    },
    /**/
    "security": {
      /*是否校验Auth（不要改）*/
      "fireBearer": false,
      /*黑洞地址（字母有错，不要介意）*/
      "blockAddress": "https://code.jquery.com/jquery-3.3.1.js",
      /*拒绝使用的令牌，Key Value一样*/
      "denyTokens": [ "*41D0C3B57CD14E3CB24E50D646440DB6_PC_chrome" ],
      /*
	Http头拒绝的规则：其中DenyType枚举如下    
        None,// 不阻止
        Hase,// 有此内容
        NonHase,// 没有此内容
        Count,// 达到数组数量
        Equals,// 内容等于
        Like,// 内容包含
        Regex// 正则匹配
	*/
      "denyHttpHeaders": [
        {
          "Head": "X-Forwarded-For", /*Http头名称*/
          "Value": "3", /*比对内容*/
          "DenyType": "Count" /*比对类型*/
        }
      ],
      /*需要检查的Api*/
      "checkApis": {
        "GoodLin-OAuth-Api/v1/oauth/getdid": {
          "Name": "GoodLin-OAuth-Api/v1/oauth/getdid",
          "Browser": "_APP",
          "Os": ""
        }
      }
    },
    /*缓存设置，Api是名称，OnlyName是指缓存时不包含Get参数*/
    "cache": [
      {
        "Api": "/GoodLin-Goods-External-Api/v1/discover/info"
      },
      {
        "Api": "/GoodLin-Goods-External-Api/v1/dashboard/advertandchannel"
      },
      {
        "Api": "/GoodLin-Goods-External-Api/v2/dashboard/hotcake"
      },
      {
        "Api": "/GoodLin-Goods-External-Api/v1/channel/info"
      } /*,
	  {
		"Api": "/Yizuan-ForcedUpdating-External-Api/v2/activty/getconfig",
		"OnlyName": true
	  }*/
    ]
  }
}
